<!-- Modal -->
<head>
  <link rel="stylesheet" href="/stylesheets/chat.css">
</head>
<div class="container">
  <div class="leftSide">
    <div class="header">
      <h2>Yakety</h2>
    </div>
    <div class="searchChat">
      <input id="findUser" class="form-control" placeholder="Find a user" autocomplete="off">
      <ul id="foundUsers">
      </ul>
    </div>
    <div class="chatlist">
      <ul id="chatsList" style="list-style: none;">
      </ul>
    </div>
  </div>
   
  <div class="rightSide">
    <div class="header">

    </div>  
    <div class="chatbox" id="messages">

    </div>
    
    <div class="chat_input">

      <textarea class="form-control" id="messageContent" rows="2" placeholder="What would you like to say?"></textarea>
      <button id="sendMessage" class="button">Send</button>
    
    </div>
  </div>
</div>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/js-cookie@3.0.1/dist/js.cookie.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/javascript/chat.js"></script>
  {{!-- <script src="/javascript/logout.js"></script> --}}
</body>

</html>
